{
  "name": "com.coremedia.labs.plugins__studio-client.content-hub-adapter-vimeo-icons",
  "version": "1.0.0",
  "description": "ContentHub Adapter Vimeo Icons",
  "private": true,
  "scripts": {
    "build": "yarn run icon-font && yarn run replace-variable && yarn run scss",
    "icon-font": "generate-icon-font ./src/main/icons ./target/unpacked-iconfont/META-INF/resources --name content-hub-adapter-vimeo-icons --sizes 16",
    "replace-variable": "replace 'extended-base-styles: true' 'extended-base-styles: false' target/unpacked-iconfont/META-INF/resources/content-hub-adapter-vimeo-icons-100/scss/_variables.scss",
    "scss": "node-sass --output-style expanded target/unpacked-iconfont/META-INF/resources/content-hub-adapter-vimeo-icons-100/scss/content-hub-adapter-vimeo-icons-100.scss target/unpacked-iconfont/META-INF/resources/content-hub-adapter-vimeo-icons-100/css/content-hub-adapter-vimeo-icons-100.css"
  },
  "devDependencies": {
    "@coremedia/icon-font-generator": "^1.0.0",
    "node-sass": "^5.0.0",
    "replace": "^1.2.1"
  },
  "dependencies": {
  }
}
